---
interface Props {
  size: string;
  color?: string;
  secondaryColor?: string;
}
const { color = "white", secondaryColor = "black", size } = Astro.props;
---
<svg
  viewBox="0 0 256 256"
  width={size}
  height={size}
  xmlns="http://www.w3.org/2000/svg"
  preserveAspectRatio="xMidYMid"
  ><path fill={color} d="M256 256H0V0h256L128 127.949z"></path></svg
>
