---
import { SOFT } from "../../data/tech";
---

<div class="grid grid-cols-2 md:grid-cols-3 gap-4 mt-2">
  {
    SOFT.map((item) => (
      <div class="inline-flex items-center justify-start md:justify-center">
        <span class="w-1.5 h-1.5 bg-black rounded-full mr-2 dark:bg-primary" />
        {item}
      </div>
    ))
  }
</div>
